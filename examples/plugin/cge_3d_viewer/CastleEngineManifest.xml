<?xml version="1.0" encoding="utf-8"?>
<!--
  Manifest file for the cge_3d_viewer project (for plugin
  or standalone application).

  Note: you could remove the plugin_source attribute below,
  and also remove the "cge_3d_viewer_plugin.lpr" file.
  The build tool can now auto-generate suitable library file,
  thanks to the game_units attribute.
-->
<project name="cge_3d_viewer"
  caption="Castle Game Engine 3D Viewer"
  qualified_name="io.castleengine.cge3dplugin"
  plugin_source="cge_3d_viewer_plugin.lpr"
  standalone_source="cge_3d_viewer_standalone.lpr"
  game_units="GameInitialize"
  author="Michalis Kamburelis">
  <version value="1.0.1" />
  <dependencies>
    <dependency name="Https" />
  </dependencies>
  <data exists="false" />
</project>
